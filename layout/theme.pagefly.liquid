    {{ 'styles-rtl.css' | asset_url | stylesheet_tag }}
    {% else %}
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    {% endif %}
    {% if settings.layout_fullpage %}
    {{ 'fullpage.css' | asset_url | stylesheet_tag }}
    {% endif %}
    <!-- Theme base and media queries -->
    {{ 'theme_exention.css' | asset_url | stylesheet_tag }}
    {{ 'styles.css' | asset_url | stylesheet_tag }}
    {{ 'owl.theme.css' | asset_url | stylesheet_tag }}
    {{ 'theme_core.scss.css' | asset_url | stylesheet_tag }}
    {{ 'responsive.css' | asset_url | stylesheet_tag }}
       {{ 'custom.css' | asset_url | stylesheet_tag }}

    {% if settings.display_csseditor %}
    {{ 'bannereditor.css' | asset_url | stylesheet_tag }}
    {% endif %}
    <!-- Scripts -->
    {% include 'head_scripts' %}
    {{ 'skrollr.js' | asset_url | script_tag }}
    {{ "tricky3.infinite-scroll.v2.min.js" | asset_url | script_tag }}
    <!-- 360 Product -->
    {{ 'threesixty.min.js' | asset_url | script_tag }}
    <!-- Header hook for plugins ================================================== -->
    {{ content_for_header }}
    {%- capture CFH -%}{{ content_for_header }}{%- endcapture -%}
    {%- if CFH contains 'design_mode' -%}
    {%- assign isAdmin = true -%}
    {%- else -%}
    {%- assign isAdmin = false -%}
    {%- endif -%}
    <!--[if lt IE 9]>
{{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
<![endif]-->

    {% comment %}
    If you store has customers disabled, you can remove the following JS file
    {% endcomment %}
    {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
    {% endif %}

    {% include 'weglot_hreftags' %}
    {% include 'weglot_switcher' %}
   
{% render 'pagefly-header' %}
 
  {% include "trackify_x" %}
</head> 

  {% comment %}
  Add the page template as a class for easy page or template specific styling.
  {% endcomment %}
  <body id="{{ page_title | handle }}" class="{% if settings.layout_width_body %}boxed-layout {% endif %}{{ settings.layout_fullpage_ancho}}{% if customer %} customer-logged-in{% endif %}{% if settings.body_rtl %} rtl{% else %} ltr{% endif %} template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}"><div class="pagefly-container">{{ content_for_layout }}</div></body>
  </html>